{
  "name": "mtgql",
  "version": "1.5.1",
  "description": "query language for searching magic: the gathering cards",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run build:card-models && npm run build:ts && npm run build:ne",
