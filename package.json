    "build"
  ],
  "homepage": "https://github.com/dsoskey/mtgql-js#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.12.0",
    "@swc/core": "1.4.0",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/moo": "0.5.9",
    "@types/nearley": "2.11.5",
    "jest": "29.7.0",
